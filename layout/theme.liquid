<!doctype html>
<html lang="{{ shop.locale }}">
  <head>
    {%- capture content_for_head -%}
    <meta charset="utf-8">
    {%- if settings.favicon != nil -%}
    <link rel="shortcut icon" type="image/x-icon" href="{{ settings.favicon | img_url: '32x32' }}">
    <link rel="apple-touch-icon" sizes="32x32" href="{{ settings.favicon | img_url: '32x32' }}">
    {%- else -%}
    <link rel="shortcut icon" type="image/x-icon" href="{{ 'favicon.png' | asset_img_url: '32x32' }}">
    <link rel="apple-touch-icon" sizes="32x32" href="{{ 'favicon.png' | asset_img_url: '32x32' }}">
    {%- endif -%}
    <meta name="HandheldFriendly" content="True">
    <meta name="MobileOptimized" content="360">
    <meta name="viewport" content="width=device-width,initial-scale=1">
     <!--  PRELOAD DNS -->
    <link rel="preconnect dns-prefetch" href="https://cdn.shopify.com" crossorigin>
    <link rel="preconnect dns-prefetch" href="//fonts.googleapis.com" crossorigin>
    <link rel="dns-prefetch" href="https://fonts.gstatic.com" crossorigin>
    <link rel="dns-prefetch" href="https://monorail-edge.shopifysvc.com" crossorigin>
    <meta name="theme-color" content="{{ settings.main_color }}">

    {%- render 'fb-open-graph' -%}

    <title>{{ page_title }}{%- if current_tags -%}{% assign meta_tags = current_tags | join: ', ' %} &ndash; {{ 'general.meta.tags' | t: tags: meta_tags }}{% endif %}{% if current_page != 1 %} &ndash; {{ 'general.meta.page' | t: page: current_page }}{% endif %}{% unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless %}</title>
    {%- if page_description -%}
    <meta name="description" content="{{ page_description | escape }}{% if current_tags %} {{ 'general.meta.tagged_html' | t: tags: meta_tags | escape }}.{% endif %}{% if current_page != 1 %} {{ 'general.meta.page' | t: page_number: current_page }}.{% endif %}" />
    {%- endif -%}
    
    <link href="{{ 'custom.css' | asset_url }}" rel="stylesheet" type="text/css" media="all">

    {%- render 'snippet-theme', type: 'root' -%}
    {%- render 'theme-font' -%}
    <link rel="preload" href="{{ 'header.min.css'   | asset_url }}" as="style">
    {%- if    template == 'index'                                       -%}<link rel="preload" href="{{ 'homepage.min.css'   | asset_url }}" as="style">
    {%- elsif template contains 'collection'                            -%}<link rel="preload" href="{{ 'collection.min.css' | asset_url }}" as="style">
    {%- elsif template contains 'product'                               -%}<link rel="preload" href="{{ 'product.min.css'    | asset_url }}" as="style">
    {%- elsif template contains 'cart'                                  -%}<link rel="preload" href="{{ 'cart.min.css'       | asset_url }}" as="style">
    {%- elsif template contains 'blog' or template contains 'article'   -%}<link rel="preload" href="{{ 'blog.min.css'       | asset_url }}" as="style">
    {%- else                                                            -%}<link rel="preload" href="{{ 'pages.min.css'      | asset_url }}" as="style">
    {%- endif -%}

    <link href="{{ 'header.min.css' | asset_url }}" rel="stylesheet" type="text/css" media="all">
    {%- endcapture -%}
    
    {%- render 'compress', content: content_for_head -%}

    {%- capture wl_cp_Include -%}
          {% include 'arn_wl_cp_settings' %}
          {% include 'arn_wl_cp_styles' %}
          {% if shop.metafields.arena.wl_cp_settings.general_settings.app_script == false %}
              <script defer crossorigin="anonymous" src="{{ 'arn-wishlist-compare.js'  | asset_url }}"></script>
          {% endif %}
    {%- endcapture -%}{%- unless wl_cp_Include contains 'Liquid error' -%}{{ wl_cp_Include }}{%- endunless -%}

    {%- render 'js-loader' -%}
    {%- render 'snippet-theme', type: 'script' -%}
    <script src="{{ 'lazysizes.min.js' | asset_url  }}" async="async"></script><script>document.addEventListener("click", x=>0)</script>

    {{content_for_header}}
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js" integrity="sha512-894YE6QWD5I59HgZOGReFYm4dnWc1Qt5NtvYSaNcOP+u1T9qYdvdihz0PPSiiqn/+/3e7Jo4EaG7TubfWGUrMQ==" crossorigin="anonymous"></script>

    {%- if settings.review_app == 'loox' -%}{{ shop.metafields.loox["global_html_head"] }}{%- endif -%}
   {% include 'qikify-navigator' %}
  {% include 'qikify-smartmenu' %}
</head>

{%- capture themeClass -%}template{{ template | split: '.' | first | camelize -}}
{%- endcapture -%}{%- assign themeClass = themeClass | strip_newlines | split: '|' | join: ' ' -%}

<body class="{{ themeClass }}" onclick tabIndex=0>
    {%- if    template == 'index'                                       -%}<link href="{{ 'homepage.min.css'        | asset_url }}" rel="stylesheet" type="text/css" media="all" >
    {%- elsif template contains 'collection'                            -%}<link href="{{ 'collection.min.css'      | asset_url }}" rel="stylesheet" type="text/css" media="all">
    {%- elsif template contains 'product'                               -%}<link href="{{ 'product.min.css'         | asset_url }}" rel="stylesheet" type="text/css" media="all">
    {%- elsif template contains 'cart'                                  -%}<link href="{{ 'cart.min.css'            | asset_url }}" rel="stylesheet" type="text/css" media="all">
    {%- elsif template contains 'blog' or template contains 'article'   -%}<link href="{{ 'blog.min.css'            | asset_url }}" rel="stylesheet" type="text/css" media="all">
    {%- else                                                            -%}<link href="{{ 'pages.min.css'           | asset_url }}" rel="stylesheet" type="text/css" media="all">
    {%- endif -%}

    {%- if settings.currency_type == '2' -%}{{- "/services/javascripts/currencies.js" | script_tag -}}<script>Loader.require('currencyJS', 'scripts');</script>{%- endif -%}

    {%- if request.design_mode -%}
        <link rel="stylesheet" href="{{ 'footer.css' | asset_url }}" as="style">
    {%- endif -%}

    {%- capture growaveSettings -%}{{- settings.wishlist_app -}}{{- settings.review_app -}}{%- endcapture -%}
    {%- if growaveSettings contains 'growave' -%}
        {% capture the_snippet_content %}{% render 'socialshopwave-helper' %}{% endcapture %}
        {% unless the_snippet_content contains 'Liquid error' %}{{ the_snippet_content }}{% endunless %}
    {%- endif -%}

    {%- capture wl_cp_Include -%}
        {%- capture content -%}{% include 'arn_icons_define' %}{%- endcapture -%}
        {%- render 'compress', content: content -%}
    {%- endcapture -%}{%- unless wl_cp_Include contains 'Liquid error' -%}{{ wl_cp_Include }}{%- endunless -%}

    {%- if settings.enable_customCss -%}{%- section 'section-css-custom' -%}{%- else -%}{%- render 'snippet-theme', type: 'themeCSS' -%}{%- endif -%}

    <div class="boxed-wrapper">

        {%- include 'header-layout' -%}

        <main id="main-content" role="main">

            {%- include 'breadcrumb', pageLayout: settings.page_layout -%}

            <div class="container-wrapper">{{ content_for_layout }}</div>

            {%- if settings.enable_iconbox -%}
                {%- if request.design_mode -%}
                    {%- section 'section-iconbox' -%}
                {%- else -%}
                    <div class="lazyqueue-wait lazyload" data-include="{{routes.root_url}}?section_id=section-iconbox"></div>
                {%- endif -%}
            {%- endif -%}
        </main><footer>
            {%- if request.design_mode -%}
                {%- if settings.enable_footerWidget -%}
                {%- section 'footer-widget' -%}{%- endif -%}
                {%- section 'footer' -%}
            {%- else -%}
                {%- if settings.enable_footerWidget -%}
                <div id="footerWidget" class="lazyqueue-wait lazyload" data-include="{{routes.root_url}}?section_id=footer-widget" data-lazyqueue></div>{%- endif -%}
                <div class="lazyqueue-wait lazyload" data-include="{{routes.root_url}}?section_id=footer" data-lazyqueue></div>
            {%- endif -%}
        </footer></div>{%- capture content -%}{%- render 'modal' -%}{%- endcapture -%}

    {%- render 'compress', content: content -%}

    {%- if settings.review_app == 'loox' -%}{{ shop.metafields.loox["global_html_body"] }}{%- endif -%}
    <script>Loader.require('tiny', 'scripts');Loader.require('arn_theme', 'scripts');
        {%- if template == 'index'              -%}Loader.require('home', 'scripts');{%- endif -%}
        {%- if template contains 'collection'   -%}Loader.require('collection', 'scripts');{%- endif -%}
        {%- if template contains 'product'      -%}Loader.require('product', 'scripts');{%- endif -%}
        {%- if template contains 'page'         -%}Loader.require('page', 'scripts');{%- endif -%}
        {%- if template contains '404'          -%}Loader.require('page_404', 'scripts');{%- endif -%}
        {%- if settings.show_quickview          -%}Loader.require('quickview', 'scripts');{%- endif -%}
        {%- if template contains 'blog' or template contains 'article' -%}Loader.require('blog', 'scripts');{%- endif -%}
        window.addEventListener('DOMContentLoaded', function(){Loader.load();})
        window.addEventListener('load', function(){console.log('load');EventTarget.prototype.addEventListener1 != null && (EventTarget.prototype.addEventListener = EventTarget.prototype.addEventListener1);})</script>
{% include 'stamped-rewards-init' %}
  <!-- Stamped - Begin Script--><script async type="text/javascript" data-api-key="" id="stamped-script-widget" src="https://cdn-stamped-io.azureedge.net/files/widget.min.js"></script><!-- Stamped - End Script --><br>
{{ 'custom.js' | asset_url | script_tag }}
</body></html>